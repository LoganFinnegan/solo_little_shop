<%= form_with model: [merchant, item], local: true, html: { id: form_id } do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :description %>
  <%= form.text_field :description %>

  <%= form.label :unit_price %>
  <%= form.number_field :unit_price %>

  <%= form.submit %>
<% end %>
